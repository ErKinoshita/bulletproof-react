{
  "name": "bulletproof-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite --port=3000",
    "build": "vite build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "postinstall": "husky install",
    "cy:install": "cypress install",
    "cy:open": "cypress open",
    "cy:headless": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 cy:open",
    "e2e:headless": "start-server-and-test start http://localhost:3000 cy:headless",
    "serve": "serve --no-clipboard --single --listen 3000 dist",
    "eject": "react-scripts eject",
    "lint": "eslint --fix --ext .js,.ts,.tsx ./src --ignore-path .gitignore",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --list-different",
    "types:check": "tsc --project tsconfig.json --pretty --noEmit",
    "validate-and-build": "npm-run-all --parallel check-types check-format lint build",
    "validate": "npm-run-all --parallel check-types && lint-staged",
    "generate": "plop",
    "storybook:open": "start-storybook -p 6006",
    "storybook:build": "build-storybook"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.2.0",
    "@heroicons/react": "^1.0.1",
    "@hookform/resolvers": "^2.5.2",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.6",
    "dompurify": "^2.2.9",
    "history": "^5.0.0",
    "intersection-observer": "^0.12.0",
    "lodash": "^4.17.21",
    "marked": "^2.0.7",
    "nanoid": "^3.1.23",
    "react": "17",
    "react-dom": "17",
    "react-error-boundary": "^3.1.3",
    "react-helmet-async": "^1.1.2",
    "react-hook-form": "^7.7.1",
    "react-query": "^3.34.15",
    "react-query-auth": "^1.0.0",
    "react-router-dom": "^6.0.0-beta.0",
    "type-fest": "^1.2.0",
    "typescript": "^4.1.2",
    "zod": "^3.1.0",
    "zustand": "^3.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@mswjs/data": "^0.3.0",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.2.6",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@tailwindcss/typography": "^0.5.8",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/dompurify": "^2.2.2",
    "@types/faker": "^5.5.7",
    "@types/jest": "^29.2.5",
    "@types/lodash": "^4.14.170",
    "@types/marked": "^2.0.3",
    "@types/react": "17",
    "@types/react-dom": "17",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/ui": "^0.26.3",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "cypress": "^7.6.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "faker": "^5.5.3",
    "husky": "^6.0.0",
    "is-ci": "^3.0.0",
    "is-ci-cli": "^2.2.0",
    "jsdom": "^20.0.3",
    "lint-staged": "^11.0.0",
    "msw": "^0.29.0",
    "npm-run-all": "^4.1.5",
    "plop": "^2.7.4",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "serve": "^12.0.0",
    "start-server-and-test": "^1.12.5",
    "tailwindcss": "^3.2.4",
    "vite": "^4.0.3",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.26.3"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
